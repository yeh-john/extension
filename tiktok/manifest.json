{
    "name": "TikTok Content Remover",
    "description": "This extension can remove TikTok recommend video.",
    "version": "0.1.0",
    "manifest_version": 3
    ,

    "content_scripts": [
        {
            "matches": [ "*://tiktok.com/*" ],
            "css": [ "style.css" ],
            "run_at": "document_start"
        }
    ],
    "host_permissions": ["*://tiktok.com/*"],
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
}
